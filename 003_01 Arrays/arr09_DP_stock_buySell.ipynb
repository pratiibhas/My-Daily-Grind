    "def best_tym(arr,n):\n",
    "    profit=0\n",
    "    mini =arr[0]\n",
    "    for i in range(n):\n",
    "        \n",
    "        cost=arr[i]-mini\n",
    "        profit=max(profit,cost)\n",
    "        mini=min(mini,arr[i])\n",
    "    return profit "
   ]
